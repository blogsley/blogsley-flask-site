{
  "name": "blogsley-flask",
  "version": "1.0.0",
  "description": "## Installation",
  "main": "index.js",
  "scripts": {
    "newenv": "python3 -m venv env",
    "setup": "pip install -r requirements.txt",
    "build": "webpack --config build/webpack.config.js",
    "buildvue": "webpack --config buildvue/webpack.config.prod.js",
    "debugbuildvue": "webpack --verbose true --config buildvue/webpack.config.prod.js",
    "freeze": "pip freeze | grep -v 'pkg-resources' > requirements.txt",
    "pipcompile": "python -m piptools compile",
    "pipupgrade": "python -m piptools compile --upgrade",
    "makemigrations": "flask db migrate",
    "migrate": "flask db upgrade",
    "deploy": "git push dokku master",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
